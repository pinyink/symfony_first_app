<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9" xmlns:xhtml="http://www.w3.org/1999/xhtml"
    xmlns:image="http://www.google.com/schemas/sitemap-image/1.1" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.sitemaps.org/schemas/sitemap/0.9 http://www.sitemaps.org/schemas/sitemap/0.9/sitemap.xsd">

    <url>
        <loc>{{ url('homepage') }}</loc>
        <lastmod>2025-03-13T14:05:33+00:00</lastmod>
        <priority>1.00</priority>
    </url>

    {% for url in urls %}
    <url>{# check if hostname is not alreay in url#}
        <loc>{{ url('blog', {'slug' : url.url}) }}</loc>
        {% if url.modified is defined %}
        <lastmod>{{url.modified|date('c')}}</lastmod>
        {% endif %}

        {# <changefreq>{{url.changefreq}}</changefreq> #}
        
        <priority>0.80</priority>
    </url>
    {% endfor %}

</urlset>